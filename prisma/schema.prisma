datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Destino {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  localizacao String?
  imagem String?
  imagem2 String?
  imagem3 String?
  imagem4 String?
  atrativos Atrativo[]

}

model Atrativo {
  id          Int      @id @default(autoincrement())
  name        String
  tipo        String
  description String?
  dicas String?

  destino_id Int
  destinos Destino @relation(fields: [destino_id], references: [id])

}

